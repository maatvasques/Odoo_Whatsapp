<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="waha_api_url" model="ir.config_parameter">
            <field name="key">whatsapp.api.url</field>
            <field name="value">http://waha:3000</field>
        </record>

        <record id="waha_api_key" model="ir.config_parameter">
            <field name="key">whatsapp.api.key</field>
            <field name="value">sua-chave-secreta-aqui</field>
        </record>

        <record id="waha_api_session" model="ir.config_parameter">
            <field name="key">whatsapp.api.session</field>
            <field name="value">default</field>
        </record>
    </data>
</odoo>